{
  "presets": [
    ["@babel/preset-env", { "targets": { "browsers": ["last 2 versions", "safari >= 7"] } }],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "css-modules-transform",
      {
        "generateScopedName": "[name]__[local]___[hash:base64:5]",
        "extensions": [".scss"]
      }
    ],
    [
      "@babel/plugin-transform-runtime"
    ],
    [
      "@babel/plugin-transform-arrow-functions"
    ],
  ]
}
